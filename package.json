{
  "name": "crypto-tracker",
  "version": "1.0.0",
  "description": "A public GraphQL crypto market data API with websocket feed",
  "sideEffects": [
    "reflect-metadata"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/saulmaldonado/CrytoTracker.git"
  },
  "main": "build/index.js",
  "scripts": {
    "build": "webpack",
    "prestart:prod": "yarn build",
    "start:prod": "node build/index.js",
    "postinstall": "patch-package",
    "test:CI": "jest --coverage --forceExit",
    "start": " tsnd --respawn ./src/index.ts",
    "prestart": "docker-compose up -d",
    "pretest": "docker-compose stop && docker-compose -f docker-compose-test.yml up -d",
    "posttest": "docker-compose stop",
    "test": "jest --coverage --logHeapUsage --forceExit",
    "lint": "eslint src/**/*.{ts}",
    "patch": "patch-package jwks-rsa @types/express-jwt"
  },
  "keywords": [],
  "engines": {"node": ">=14.8"},
  "author": "Saul Maldonado <samaldonado4@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@typegoose/typegoose": "^7.3.5",
    "apollo-server-express": "^2.17.0",
    "auth0-js": "^9.14.0",
    "axios": "^0.21.0",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "graphql": "^15.3.0",
    "graphql-redis-subscriptions": "^2.3.1",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^1.9.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.5",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "qs": "^6.9.4",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.0.0",
    "type-graphql": "^1.0.0",
    "uuid": "^8.3.0",
    "weak-napi": "^2.0.2",
    "webpack-node-externals": "^2.5.2"
  },
  "devDependencies": {
    "@types/auth0-js": "^9.14.0",
    "@types/babel__core": "^7.1.9",
    "@types/express": "^4.17.8",
    "@types/ioredis": "^4.17.4",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.27",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.10.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "apollo-server-testing": "^2.17.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.4",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,yaml,yml}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
